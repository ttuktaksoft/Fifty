<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fifty.fiftyhouse.com.fifty">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activty.LoadingActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:parentActivityName=".activty.LoadingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/NoTitleBar" />
        <activity android:name=".activty.LoginActivity" />
        <activity android:name=".activty.SignUp.SignUpActivity" />
        <activity android:name=".activty.SignUp.BirthActivity" />
        <activity android:name=".activty.SignUp.FavoriteActivity" />
        <activity android:name=".activty.SignUp.ProfileImgActivity"></activity>
    </application>

</manifest>