<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activty.Profile.UserProfileActivity"
    android:background="@color/at_user_profile_bg">


    <android.support.v4.widget.NestedScrollView
        android:id="@+id/ns_UserProfile_Scroll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none"
        android:background="@color/ns_UserProfile_Scroll"
        app:layout_constraintBottom_toTopOf="@+id/v_UserProfile_BottomBar">

        <android.support.constraint.ConstraintLayout
            android:id="@+id/v_UserProfile_Info"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            >

            <ImageView
                android:id="@+id/iv_UserProfile_Profile"
                android:layout_width="200dp"
                android:layout_height="200dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toTopOf="@+id/v_UserProfile_Info"
                android:layout_marginTop="96dp"
                android:src="@drawable/bg_empty_circle"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Name"
                style="@style/BaseTextView"
                android:layout_width="0dp"
                android:layout_height="50dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@+id/iv_UserProfile_Profile"
                android:layout_marginTop="15dp"
                android:gravity="center"
                android:text="닉네임"
                android:textSize="25dp"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Age"
                style="@style/BaseTextView"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@+id/tv_UserProfile_Info_Name"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:text="20세"
                android:textSize="15dp"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Gender"
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:layout_constraintRight_toLeftOf="@+id/tv_UserProfile_Info_Age"
                app:layout_constraintTop_toTopOf="@+id/tv_UserProfile_Info_Age"
                app:layout_constraintBottom_toBottomOf="@+id/tv_UserProfile_Info_Age"
                android:layout_marginRight="10dp"
                android:src="@drawable/ic_man_simple"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Favorite_Line_1"
                android:layout_width="0dp"
                android:layout_height="2dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@id/tv_UserProfile_Info_Age"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp"
                android:alpha="0.5"
                android:background="@color/gray_light"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Favorite"
                style="@style/BaseTextView"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@+id/iv_UserProfile_Info_Favorite_Line_1"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:textSize="20dp"
                android:text="축구, 배틀그라운드"
                android:gravity="center"
                />
            <ImageView
                android:id="@+id/iv_UserProfile_Info_Favorite_Line_2"
                android:layout_width="0dp"
                android:layout_height="2dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@id/tv_UserProfile_Info_Favorite"
                android:alpha="0.5"
                android:background="@color/gray_light"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Memo_BG"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintBottom_toBottomOf="@+id/tv_UserProfile_Info_Memo"
                app:layout_constraintTop_toTopOf="@+id/tv_UserProfile_Info_Memo"
                android:background="@color/iv_UserProfile_Info_Memo_BG"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Memo_Line"
                android:layout_width="0dp"
                android:layout_height="2dp"
                app:layout_constraintRight_toRightOf="@+id/iv_UserProfile_Info_Memo_BG"
                app:layout_constraintLeft_toLeftOf="@+id/iv_UserProfile_Info_Memo_BG"
                app:layout_constraintBottom_toBottomOf="@+id/iv_UserProfile_Info_Memo_BG"
                android:alpha="0.5"
                android:background="@color/gray_light"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Memo"
                style="@style/BaseTextView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:minHeight="40dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@+id/iv_UserProfile_Info_Favorite_Line_2"
                android:paddingRight="40dp"
                android:paddingLeft="20dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp"
                android:gravity="center"
                android:maxLines="2"
                android:text="자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개자기소개"
                android:textSize="20dp"/>



            <ImageView
                android:id="@+id/iv_UserProfile_Info_Memo_Button"
                android:layout_width="20dp"
                android:layout_height="20dp"
                app:layout_constraintRight_toRightOf="@+id/iv_UserProfile_Info_Memo_BG"
                app:layout_constraintTop_toTopOf="@+id/iv_UserProfile_Info_Memo_BG"
                app:layout_constraintBottom_toBottomOf="@+id/iv_UserProfile_Info_Memo_BG"
                android:layout_marginRight="10dp"
                android:scaleX="-1"
                android:src="@drawable/ic_back"
                />



            <ImageView
                android:id="@+id/iv_UserProfile_Info_Count_BG"
                android:layout_width="0dp"
                android:layout_height="40dp"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@+id/tv_UserProfile_Info_Memo"
                android:background="@color/iv_UserProfile_Info_Count_BG"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Count_BG_Line"
                android:layout_width="0dp"
                android:layout_height="2dp"
                app:layout_constraintRight_toRightOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintLeft_toLeftOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintBottom_toBottomOf="@+id/iv_UserProfile_Info_Count_BG"
                android:alpha="0.5"
                android:background="@color/gray_light"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Count_Visit"
                style="@style/BaseTextView"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintLeft_toLeftOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintRight_toLeftOf="@+id/tv_UserProfile_Info_Count_Like"
                app:layout_constraintTop_toTopOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintBottom_toBottomOf="@+id/iv_UserProfile_Info_Count_BG"
                android:gravity="center"
                android:text="방문자 10 / 10k"
                android:textSize="15dp"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Count_Line_1"
                android:layout_width="2dp"
                android:layout_height="0dp"
                app:layout_constraintRight_toRightOf="@+id/tv_UserProfile_Info_Count_Visit"
                app:layout_constraintTop_toTopOf="@+id/tv_UserProfile_Info_Count_Visit"
                app:layout_constraintBottom_toBottomOf="@+id/tv_UserProfile_Info_Count_Visit"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp"
                android:alpha="0.5"
                android:background="@color/gray_light"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Count_Like"
                style="@style/BaseTextView"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintLeft_toRightOf="@+id/tv_UserProfile_Info_Count_Visit"
                app:layout_constraintRight_toLeftOf="@+id/tv_UserProfile_Info_Count_Near"
                app:layout_constraintTop_toTopOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintBottom_toBottomOf="@+id/iv_UserProfile_Info_Count_BG"
                android:gravity="center"
                android:text="좋아요 10 / 1k"
                android:textSize="15dp"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Count_Near"
                style="@style/BaseTextView"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintLeft_toRightOf="@+id/tv_UserProfile_Info_Count_Like"
                app:layout_constraintRight_toRightOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintTop_toTopOf="@+id/iv_UserProfile_Info_Count_BG"
                app:layout_constraintBottom_toBottomOf="@+id/iv_UserProfile_Info_Count_BG"
                android:gravity="center"
                android:text="거리 10km"
                android:textSize="15dp"
                />

            <ImageView
                android:id="@+id/iv_UserProfile_Info_Count_Line_2"
                android:layout_width="2dp"
                android:layout_height="0dp"
                app:layout_constraintLeft_toLeftOf="@+id/tv_UserProfile_Info_Count_Near"
                app:layout_constraintTop_toTopOf="@+id/tv_UserProfile_Info_Count_Near"
                app:layout_constraintBottom_toBottomOf="@+id/tv_UserProfile_Info_Count_Near"
                android:layout_marginTop="3dp"
                android:layout_marginBottom="3dp"
                android:alpha="0.5"
                android:background="@color/gray_light"
                />

            <TextView
                android:id="@+id/tv_UserProfile_Info_Photo"
                style="@style/BaseTextView"
                android:layout_width="0dp"
                android:layout_height="30dp"
                app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_Info"
                app:layout_constraintRight_toRightOf="@+id/v_UserProfile_Info"
                app:layout_constraintTop_toBottomOf="@+id/iv_UserProfile_Info_Count_BG"
                android:paddingLeft="20dp"
                android:text="@string/tv_UserProfile_Info_Photo"
                android:gravity="center_vertical"
                />


            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_UserProfile_Info_Photo"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toBottomOf="@+id/tv_UserProfile_Info_Photo"
                android:nestedScrollingEnabled="false"
                android:paddingBottom="20dp"/>

        </android.support.constraint.ConstraintLayout>

    </android.support.v4.widget.NestedScrollView>

    <android.support.constraint.ConstraintLayout
        android:id="@+id/v_UserProfile_TopBar"
        android:layout_width="match_parent"
        android:layout_height="56dp"
        android:background="@color/v_UserProfile_TopBar">

        <ImageView
            android:id="@+id/iv_UserProfile_Back"
            android:layout_width="30dp"
            android:layout_height="30dp"
            app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_TopBar"
            app:layout_constraintTop_toTopOf="@+id/v_UserProfile_TopBar"
            app:layout_constraintBottom_toBottomOf="@+id/v_UserProfile_TopBar"
            android:layout_marginLeft="10dp"
            android:src="@drawable/ic_back"
            />

        <ImageView
            android:id="@+id/iv_UserProfile_Alert"
            android:layout_width="35dp"
            android:layout_height="35dp"
            app:layout_constraintRight_toRightOf="@+id/v_UserProfile_TopBar"
            app:layout_constraintTop_toTopOf="@+id/v_UserProfile_TopBar"
            app:layout_constraintBottom_toBottomOf="@+id/v_UserProfile_TopBar"
            android:layout_marginRight="10dp"
            android:src="@drawable/ic_alert"
            />

        <TextView
            android:id="@+id/tv_UserProfile_Name"
            style="@style/BaseTextView"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintRight_toLeftOf="@+id/iv_UserProfile_Alert"
            app:layout_constraintLeft_toRightOf="@+id/iv_UserProfile_Back"
            app:layout_constraintTop_toTopOf="@+id/v_UserProfile_TopBar"
            app:layout_constraintBottom_toBottomOf="@+id/v_UserProfile_TopBar"
            android:gravity="center"
            android:text="닉네임"
            android:textSize="25dp"
            />

    </android.support.constraint.ConstraintLayout>




    <android.support.constraint.ConstraintLayout
        android:id="@+id/v_UserProfile_BottomBar"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="@color/v_UserProfile_BottomBar"
        app:layout_constraintBottom_toBottomOf="parent">

        <android.support.constraint.ConstraintLayout
            android:id="@+id/v_UserProfile_BottomBar_Like"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintRight_toLeftOf="@+id/v_UserProfile_BottomBar_Chat"
            app:layout_constraintLeft_toLeftOf="@+id/v_UserProfile_BottomBar"
